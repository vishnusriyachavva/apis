        <!-- <button id="btn" >Show data</button> -->
    </form>
       
<br><br>

           <table class="table table-borderless">
    <thead>
        <tr class="th">
            <th>#</th>
            <th>name</th>
            <th>Last</th>
            <th>Buy/Sell_Price</th>
            <th>Volume</th>
            <th>base_unit</th>
        </tr>
    </thead>
    <% for(let i=0;i<responseArr.length;i++){ %>
    <tr class="ro">
        <td><%= i+1 %></td>
        <td><%= responseArr[i].name%></td>
        <td>₹<%= responseArr[i].last%></td>
        <td>₹<%= responseArr[i].buy%>/<%= responseArr[i].sell%></td>
        <td><%= responseArr[i].volume%></td>
        <td><%= responseArr[i].base_unit%></td>
    </tr>
  <!-- <br> -->
  <tr class="empt"></tr>
    <% } %>
    
</table>


